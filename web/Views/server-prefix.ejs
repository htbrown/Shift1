        integrity="sha256-WpOohJOqMqqyKL9FccASB9O0KwACQJpFTUBLTYOVvVU="
        crossorigin="anonymous"
    ></script>

    <script>
        function submit(serverID) {
            let prefix = document.getElementById('prefixForm').value;

            if (prefix) {
                $.post('/api/prefix', {
                    prefix,
                    serverID
                })

                setTimeout(() => {
                    window.location.reload();
                }, 3000);
            } else {
                console.log('jeff')
            }
        }
    </script>
</head>
